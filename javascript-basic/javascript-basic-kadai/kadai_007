<body>
 <script>
	let num = Math.floor(Math.random()*51)+10;
	if ( num *3 ){
		console.log('3の倍数です');
	}
	else if( num * 5 ){
		console.log('5の倍数です');
	}
	else if(num * 5 == num * 3 ){
		console.log('3と５の倍数です');
	}else{
		console.log('それ以外です');
	}
 </script>
